{
  "name": "the-life-museum",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "copy:api": "python3 scripts/copy_api_blueprint.py --src app/api-blueprint --dst app/api || python scripts/copy_api_blueprint.py --src app/api-blueprint --dst app/api",
    "copy:api:dry": "python3 scripts/copy_api_blueprint.py --src app/api-blueprint --dst app/api --dry-run --verbose || python scripts/copy_api_blueprint.py --src app/api-blueprint --dst app/api --dry-run --verbose",
    "copy:api:skip": "python3 scripts/copy_api_blueprint.py --src app/api-blueprint --dst app/api --skip-existing || python scripts/copy_api_blueprint.py --src app/api-blueprint --dst app/api --skip-existing",
    "copy:api:win": "py -3 scripts/copy_api_blueprint.py --src app/api-blueprint --dst app/api",
    "copy:api:win:dry": "py -3 scripts/copy_api_blueprint.py --src app/api-blueprint --dst app/api --dry-run --verbose",
    "copy:api:win:skip": "py -3 scripts/copy_api_blueprint.py --src app/api-blueprint --dst app/api --skip-existing",
    "predev": "npm run copy:api",
    "prebuild": "npm run copy:api",
    "prestart": "npm run copy:api"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.917.0",
    "@aws-sdk/s3-request-presigner": "^3.917.0",
    "@prisma/client": "^6.17.1",
    "@prisma/extension-accelerate": "^2.0.2",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "aws-sdk": "^2.1692.0",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "next": "15.5.6",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma-erd-generator": "^2.1.0",
    "tailwindcss": "^4.1.16",
    "wrangler": "^4.45.0"
  }
}
